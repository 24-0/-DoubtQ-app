{
  "deno.enable": true,
  "deno.enablePaths": ["./src/supabase/functions"],
  "deno.config": "./src/supabase/functions/my-function/deno.json",
  "deno.importMap": "./src/supabase/functions/my-function/import_map.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://esm.sh": true
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.suggest.autoImports": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
